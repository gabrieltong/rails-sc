<h1>Listing baomings</h1>

<table>
  <tr>
    <th>Person</th>
    <th>Discount way</th>
    <th>Payment way</th>
    <th>Makeup</th>
    <th>Next</th>
    <th>Important</th>
    <th>Installment</th>
    <th>Installment duration</th>
    <th>Installment value</th>
    <th>Installment last</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @baomings.each do |baoming| %>
  <tr>
    <td><%= baoming.person_id %></td>
    <td><%= baoming.discount_way %></td>
    <td><%= baoming.payment_way %></td>
    <td><%= baoming.makeup %></td>
    <td><%= baoming.next %></td>
    <td><%= baoming.important %></td>
    <td><%= baoming.installment %></td>
    <td><%= baoming.installment_duration %></td>
    <td><%= baoming.installment_value %></td>
    <td><%= baoming.installment_last %></td>
    <td><%= link_to 'Show', baoming %></td>
    <td><%= link_to 'Edit', edit_baoming_path(baoming) %></td>
    <td><%= link_to 'Destroy', baoming, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Baoming', new_baoming_path %>
