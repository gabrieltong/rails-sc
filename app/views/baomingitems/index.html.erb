<h1>Listing baomingitems</h1>

<table>
  <tr>
    <th>Baoming</th>
    <th>Free</th>
    <th>Klass</th>
    <th>End at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @baomingitems.each do |baomingitem| %>
  <tr>
    <td><%= baomingitem.baoming_id %></td>
    <td><%= baomingitem.free %></td>
    <td><%= baomingitem.klass_id %></td>
    <td><%= baomingitem.end_at %></td>
    <td><%= link_to 'Show', baomingitem %></td>
    <td><%= link_to 'Edit', edit_baomingitem_path(baomingitem) %></td>
    <td><%= link_to 'Destroy', baomingitem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

