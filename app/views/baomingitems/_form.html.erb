<%= form_for @baomingitem, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :baoming_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :baoming_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :free, :class => 'control-label' %>
    <div class="controls">
      <%= f.check_box :free, :class => 'check_box' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :klass_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :klass_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :end_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.datetime_select :end_at, :class => 'datetime_select' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                baomingitems_path, :class => 'btn' %>
  </div>
<% end %>
